    <validationRules>
        <fullName>Confirm_Adjustment_on_change_Country</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(OP_Country__c )&amp;&amp; Confirm_Adjustment_for_Change_of_Country__c != TRUE</errorConditionFormula>
        <errorMessage>Please confirm that you have adjusted the Scholarship Amounts below after the Country has changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Digital_Marketing_Campuses</fullName>
        <active>false</active>
        <description>This validation rule will ensure that the Digital Marketing Specialization is only selected for MIM Programs on campuses where it is offered.</description>
        <errorConditionFormula>AND (
     CONTAINS(TEXT(Specializations__c),&apos;MIM-Digital Marketing Specialization&apos;),  
     OP_Program__c !=&apos;MIM-US-San Francisco&apos;,
     OP_Program__c !=&apos;MIM-UK-London&apos;
   )</errorConditionFormula>
        <errorMessage>The Digital Marketing Specialization is only offered with MIM Programs in San Francisco and London</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dubai_Housing_Bursary_only_MBA</fullName>
        <active>true</active>
        <description>Only MBA-UAE-Dubai can choose the option &quot;Dubai Housing Bursary&quot; on Housing Accomodation</description>
        <errorConditionFormula>Program__c&lt;&gt; &quot;MBA-UAE-Dubai&quot; &amp;&amp;   text(Housing_Accommodation__c) =&quot;Dubai Housing Bursary&quot;</errorConditionFormula>
        <errorDisplayField>Housing_Accommodation__c</errorDisplayField>
        <errorMessage>&quot;Dubai Housing Bursary&quot; Housing Accomodation is only available to MBA-UAE-Dubai program</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMBA_Hotel_only_EMBA</fullName>
        <active>true</active>
        <description>&quot;EMBA Hotel&quot; Housing Accomodation is only available to EMBA programs</description>
        <errorConditionFormula>not(contains(Program__c, &quot;EMBA&quot;)) &amp;&amp;   text(Housing_Accommodation__c) =&quot;EMBA Hotel&quot;</errorConditionFormula>
        <errorDisplayField>Housing_Accommodation__c</errorDisplayField>
        <errorMessage>&quot;EMBA Hotel&quot; Housing Accomodation is only available to EMBA programs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Financing_Status_validation</fullName>
        <active>true</active>
        <description>Recruiters can only change this to &apos;Applied&apos; and Finance can also pick &apos;Approved&apos; and &apos;Rejected &apos;</description>
        <errorConditionFormula>if ((ispickval(External_Financing_Status__c,&quot;Approved&quot;)  || ispickval(External_Financing_Status__c,&quot;Rejected&quot;)) &amp;&amp; $Profile.Name = &quot;1.2SU Hult Recruiter&quot;,TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>External_Financing_Status__c</errorDisplayField>
        <errorMessage>Recruiters can only set this field to the value: &quot;Applied&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FlexiPlan_only_for_EMBA</fullName>
        <active>false</active>
        <description>Users can only select FlexiPlan or No FlexiPlan when the program is EMBA</description>
        <errorConditionFormula>if ((ispickval(Installment_Plan__c,&quot;No Installment Plan&quot;) || ispickval(Installment_Plan__c,&quot;Installment Plan&quot;)) &amp;&amp; CONTAINS( Program__c ,&apos;EMBA&apos;) , TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Installment_Plan__c</errorDisplayField>
        <errorMessage>Installment Plan only applies to non EMBA programs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Housing_Not_Available_for_this_Program</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Program__c&lt;&gt; &quot;MBA-China-Shanghai&quot;, Program__c&lt;&gt; &quot;Masters-China-Shanghai&quot;) &amp;&amp;   text(Housing_Accommodation__c) =&quot;Shanghai - Single Room (1 Person) - 8 Months - Free&quot;</errorConditionFormula>
        <errorDisplayField>Housing_Accommodation__c</errorDisplayField>
        <errorMessage>Housing Accomodations are not available for this Program or Campus  Please Clear the Housing Accomodation Field and Save Again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Installment_Plan_only_for_non_EMBA</fullName>
        <active>true</active>
        <description>Users can only select Installment Plan or No Installment Plan when the program is not EMBA</description>
        <errorConditionFormula>if ((ispickval(Installment_Plan__c,&quot;No FlexiPlan&quot;) || ispickval(Installment_Plan__c,&quot;FlexiPlan&quot;)) &amp;&amp; FIND(&quot;EMBA&quot;,Program__c,0)=0 &amp;&amp;  $Profile.Name &lt;&gt; &quot;System Administrator&quot;, TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Installment_Plan__c</errorDisplayField>
        <errorMessage>FlexiPlan only applies to EMBA programs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Be_Blank_Dubai_Housing_Fee</fullName>
        <active>true</active>
        <errorConditionFormula>&apos;United Arab Emirates&apos; != Country__c &amp;&amp; NOT(ISBLANK(Dubai_Housing_Fee__c))</errorConditionFormula>
        <errorDisplayField>Dubai_Housing_Fee__c</errorDisplayField>
        <errorMessage>Only applies to the United Arab Emirates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Match_Program_Currency</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Opportunity__r.Program__r.CurrencyIsoCode) != TEXT(CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The finance currency must match the related program&apos;s currency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Option_Not_Currrently_Available</fullName>
        <active>false</active>
        <description>The Insurance Option you have chosen is not currenlty available.  Please select another</description>
        <errorConditionFormula>AND(!ISPICKVAL(Insurance__c, &quot;Waived&quot;),!ISPICKVAL(Insurance__c, &quot;Single&quot;) )</errorConditionFormula>
        <errorDisplayField>Insurance__c</errorDisplayField>
        <errorMessage>The Insurance Option you have chosen is not currenlty available.  Please select another</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partner_Program_Not_Available</fullName>
        <active>true</active>
        <description>Partner Programs are only available at certain locations and in conjunction with Master Programs.</description>
        <errorConditionFormula>AND(
    NOT (
         ISBLANK(
              TEXT(EF_Partner_Programs__c )
                          )
              ),
     TEXT(Opportunity__r.Primary_Campus__c) !=&apos;London&apos;,
     TEXT(Opportunity__r.Primary_Campus__c) !=&apos;Boston&apos;,
       !CONTAINS(OP_Program__c,&apos;MFIN&apos;),
       !CONTAINS(OP_Program__c,&apos;MIB&apos;),
       !CONTAINS(OP_Program__c,&apos;MSE&apos;),
       !CONTAINS(OP_Program__c,&apos;MIM&apos;)

)</errorConditionFormula>
        <errorMessage>The IEMA Program is only available with Master Programs at Particular Campuses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Relocation_Guarantee</fullName>
        <active>true</active>
        <description>Only editable when program is changed from MIM-Dubai(a0JU00000009JmF) orMFIN-Shanghai (a0JU00000009JmI) to MIB-Any</description>
        <errorConditionFormula>if(  (AND(OP_Previous_Program__c&lt;&gt; &quot;a0JU00000009JmF&quot;,
OP_Previous_Program__c&lt;&gt; &quot;a0JU00000009JmI&quot;)   ||  MID(Program__c,1,3) &lt;&gt; &quot;MIB&quot;) &amp;&amp;  Relocation_Guarantee__c=TRUE, true, false)</errorConditionFormula>
        <errorDisplayField>Relocation_Guarantee__c</errorDisplayField>
        <errorMessage>Rebooking Guarantee (MIB) is only available on MIM-UAE-Dubai or MFIN-China-Shanghai being changed to any MIB program. 

If your intention is to make a program change, please go to the finance object and untick the checkbox [Rebooking Guarantee (MIB)]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Erika_To_CN_UAE_UK</fullName>
        <active>false</active>
        <errorConditionFormula>Opportunity__r.Program__r.Country__r.Alpha_3__c != &apos;UAE&apos; &amp;&amp; Opportunity__r.Program__r.Country__r.Alpha_2__c != &apos;CN&apos; &amp;&amp;
Opportunity__r.Program__r.Country__r.Alpha_2__c != &apos;UK&apos; &amp;&amp; 
Insurance_Provider__c=&apos;Erika&apos;</errorConditionFormula>
        <errorDisplayField>Erika_Insurance__c</errorDisplayField>
        <errorMessage>Erika Insurance only applies to programs in the either China, the United Arab Emirates, or the United Kingdom.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Visa_Fee_only_for_Dubai</fullName>
        <active>true</active>
        <errorConditionFormula>((NOT(ISBLANK( Visa_Type__c ) )) || Contains(Visa_Type__c, &apos;None&apos;)) &amp;&amp; OP_Country__c !=&apos;United Arab Emirates&apos;</errorConditionFormula>
        <errorMessage>Visa Fee only required for Dubai</errorMessage>
    </validationRules>
