    <validationRules>
        <fullName>Invalid_Opportunity_On_Template</fullName>
        <active>true</active>
        <errorConditionFormula>OR(NOT(ISBLANK(Department__c)), NOT(ISBLANK(Program__c)), NOT(ISBLANK(Country__c))) &amp;&amp; NOT(ISBLANK(Opportunity__c))</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Internal or integration error: Template requirements cannot also be related to a specific opportunity. A template requirement is one that is directly associated with a Department, Country, or Program. Please remove the Opportunity relationship.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Parenting</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Parent__r.Parent__c))</errorConditionFormula>
        <errorDisplayField>Parent__c</errorDisplayField>
        <errorMessage>Internal or integration error: Invalid parenting: a requirement cannot be a child of another child requirement. The top level requirement is the template, and the immediate children are mapped to opportunities. There is no valid third level structure.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Parenting_Opportunity_Missing</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Parent__c)) &amp;&amp; ISBLANK(Opportunity__c)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Internal or integration error: All requirements belonging to an opportunity must also mapped to a parent requirement, and vice-versa.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Parenting_Opportunity_Present</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Parent__c) &amp;&amp; NOT(ISBLANK(Opportunity__c)) &amp;&amp; ISBLANK(Contact__c)</errorConditionFormula>
        <errorDisplayField>Parent__c</errorDisplayField>
        <errorMessage>Internal or integration error: All requirements belonging to an opportunity (and not linked to a contact) must also mapped to a parent requirement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mismatched_Program_And_Country</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Parent__r.Country__c)), Parent__r.Country__c != Opportunity__r.Program__r.Country__c)</errorConditionFormula>
        <errorDisplayField>Parent__c</errorDisplayField>
        <errorMessage>Internal or integration error: The Opportunity&apos;s Program&apos;s Country does not match the Parent Requirement&apos;s Country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mismatched_Program_And_Department</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Parent__r.Department__c)), Parent__r.Department__c != Opportunity__r.Program__r.Department__c) &amp;&amp;  $Setup.Setting_Disabled__c.Validate_Requirement_Program__c != true</errorConditionFormula>
        <errorDisplayField>Parent__c</errorDisplayField>
        <errorMessage>Internal or integration error: The Opportunity&apos;s Program&apos;s Department does not match the Parent Requirement&apos;s Department.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Requirement_Validation</fullName>
        <active>false</active>
        <description>a single requirement can only be associated to either a country or a department or a Program at a time</description>
        <errorConditionFormula>OR( 
       AND(   NOT( ISBLANK( Country__c )) , NOT( ISBLANK(  Department__c  ))    ),     
      AND(    NOT( ISBLANK( Program__c )) , NOT( ISBLANK( Department__c ))     ),   
      AND(    NOT( ISBLANK(Country__c))  , NOT( ISBLANK(Program__c))                ), 
      AND(    NOT( ISBLANK(Country__c))  ,  NOT( ISBLANK(Department__c  )),  NOT( ISBLANK( Program__c))      ) 
)</errorConditionFormula>
        <errorMessage>A single requirement can only be associated to either a country or a department or a Program at a time.</errorMessage>
    </validationRules>
