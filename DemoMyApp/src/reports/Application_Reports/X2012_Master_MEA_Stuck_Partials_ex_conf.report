    <currency>USD</currency>
    <description>This report gives an overview of all 2012 MEA Master Partials, Under Review, AEs, Cond. Accepted, Accepted which are stuck. This report shows applications which are on their status for more than 20 days and need follow up by the recruiter.</description>
    <filter>
        <criteriaItems>
            <column>Opportunity.Start_Term__c</column>
            <operator>equals</operator>
            <value>September 2012</value>
        </criteriaItems>
        <criteriaItems>
            <column>STAGE_NAME</column>
            <operator>equals</operator>
            <value>Partial Application,Waiting for Review,Under Review,Endorsed,Accepted,Rejected</value>
        </criteriaItems>
        <criteriaItems>
            <column>STAGE_DURATION</column>
            <operator>greaterThan</operator>
            <value>20</value>
        </criteriaItems>
        <criteriaItems>
            <column>Opportunity.Program__c</column>
            <operator>startsWith</operator>
            <value>MIB,MSE,MIM,MFIN</value>
        </criteriaItems>
        <criteriaItems>
            <column>Opportunity.Region__c</column>
            <operator>startsWith</operator>
            <value>mea</value>
        </criteriaItems>
    </filter>
    <format>Tabular</format>
    <name>2012 Master MEA Stuck Partials ex conf</name>
    <params>
        <name>open</name>
        <value>all</value>
    </params>
    <params>
        <name>probability</name>
        <value>&gt;0</value>
    </params>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>Opportunity</reportType>
    <scope>organization</scope>
    <showDetails>true</showDetails>
    <sortColumn>Opportunity.Country_Of_Residence__c</sortColumn>
    <sortOrder>Asc</sortOrder>
    <timeFrameFilter>
