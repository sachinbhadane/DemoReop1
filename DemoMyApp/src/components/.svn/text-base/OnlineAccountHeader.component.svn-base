    <td align="center" width="88" id="Exam" style="background-image:url({!URLFOR($Resource.Images, 'new-images/tabimage.jpg')});background-repeat:no-repeat" class="headertab">
    <apex:outputLink styleClass="customlink" value="/ApplicantExamInfoOnline"><span id="Exam1">Exam <br />Information</span></apex:outputLink></td>
    <td align="center" width="88" id="CheckReverences" style="background-image:url({!URLFOR($Resource.Images, 'new-images/tabimage.jpg')}) ;;background-repeat:no-repeat" class="headertab">
    <apex:outputLink styleClass="customlink" value="/CheckReverences"><span id="CheckReverences1">References</span></apex:outputLink></td>    
     <td align="center" width="88" id="Attach" style="background-image:url({!URLFOR($Resource.Images, 'new-images/tabimage.jpg')});background-repeat:no-repeat" class="headertab">
    <apex:outputLink styleClass="customlink" value="/AttachDocs"><span id="Attach1">Attach Documents</span></apex:outputLink></td>

        
    <td align="center" width="88" id="Personal" style="background-image:url({!URLFOR($Resource.Images, 'new-images/tabimage.jpg')});background-repeat:no-repeat ; " class="headertab">
    <apex:outputLink styleclass="customlink" target="blank"  value="/ApplicantProgramLocation"><span id="Personal1">Review Full Application</span></apex:outputLink></td>    
    
   
   <td align="center" valign="top" width="250" >
   <table width="100%" height="70" border="0" style="background-color:#E0E0E0" >
       <tr>
           <td>
           </td>
       </tr>
   </table>
